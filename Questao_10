#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#define TAM 5
#define MX 100
int main() {
    int num[TAM][TAM],n,cont=0;
    
    srand(time(NULL));
    for(int i = 0; i<TAM;i++){
        for(int j = 0; j<TAM;j++){
            num[i][j] = rand() % MX;
        }
    }
    printf("Valores atuais: \n");
    for(int i = 0; i<TAM;i++){
        for(int j = 0;j<TAM;j++){
            printf("|%2d| %t",num[i][j]);
        }
        printf("\n");
    }
    puts("Digite algum nÃºmero: \n");
    scanf("%d",&n);
    for(int i = 0;i<TAM;i++){
        for(int j=0;j<TAM;j++){
            if(n == num[i][j]){
                cont++;
            }
        }
    }
    printf("Contagem: %d",cont);

    return 0;
}
